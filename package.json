{
  "name": "mocha-benchmark2",
  "version": "1.0.2",
  "description": "Mocha + Benchmark + Chai, support sync and async test",
  "keywords": [
    "assert",
    "assertion",
    "async",
    "bdd",
    "benchmark",
    "chai",
    "cli",
    "debug",
    "debug configuration",
    "expect",
    "idea",
    "intellij",
    "intellij idea",
    "microtime",
    "mocha",
    "mocha-benchmark",
    "performance",
    "promise",
    "run configuration",
    "should",
    "speed",
    "sync",
    "tap",
    "tdd",
    "test",
    "testing"
  ],
  "homepage": "https://github.com/bluelovers/mocha-benchmark#readme",
  "bugs": {
    "url": "https://github.com/bluelovers/mocha-benchmark/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bluelovers/mocha-benchmark.git"
  },
  "license": "MIT",
  "author": "James Lal [:lightsofapollo]",
  "main": "mocha_benchmark.js",
  "types": [
    "./global.d.ts"
  ],
  "bin": {
    "mocha-benchmark2": "./bin/mocha-benchmark-cli.js"
  },
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "demo": "mocha --require ./register test/demo.test.demo.js",
    "test": "mocha --require ts-node/register --require ./register \"!(node_modules|demo)/**/*.+(test|spec).ts\""
  },
  "dependencies": {
    "cross-spawn": "^6.0.5",
    "mocha-logger": "^1.0.6",
    "optional-require": "^1.0.0",
    "yargs2": "^1.0.0"
  },
  "devDependencies": {
    "@types/benchmark": "^1.0.31",
    "@types/chai": "^4.1.3",
    "@types/mocha": "^5.2.1",
    "@types/node": "^10.3.0"
  },
  "peerDependencies": {
    "benchmark": "^2.1.4",
    "chai": "^4.1.2",
    "microtime": "^2.1.8",
    "mocha": "^5.2"
  }
}
